<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Choose>
    <When Condition="'$(UseAotMode)' == 'true'">
      <ItemGroup>
        <Reference Include="Parlot">
          <HintPath>$(MSBuildThisFileDirectory)..\..\assemblies\net9.0\Parlot.AOT.dll</HintPath>
        </Reference>
      </ItemGroup>
    </When>
    <Otherwise>
      <ItemGroup>
        <Reference Include="Parlot">
          <HintPath>$(MSBuildThisFileDirectory)..\..\assemblies\net9.0\Parlot.dll</HintPath>
        </Reference>
      </ItemGroup>
    </Otherwise>
  </Choose>
</Project>